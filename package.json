{
  "name": "Gyazo-Extension",
  "version": "2.4.4",
  "dependencies": {
    "bowser": "^1.3.0",
    "chrome-browser-object-polyfill": "^0.2.2",
    "delegate": "^3.0.0",
    "dom-css": "^2.0.0",
    "jquery": "^3.2.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "browserify": "^14.3.0",
    "chrome-webstore-manager": "^0.4.1",
    "copy-webpack-plugin": "^4.0.1",
    "crx": "^3.2.1",
    "jsdom": "^9.12.0",
    "manifoldjs": "^0.7.5",
    "npm-run-all": "^4.0.2",
    "on-build-webpack": "^0.1.0",
    "request-promise": "^4.2.0",
    "standard": "^10.0.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "web-ext": "^1.0.1",
    "webpack": "^2.4.1",
    "wemf": "^1.0.0"
  },
  "scripts": {
    "test": "standard \"./**/*.js\" && ava",
    "clean": "rm -rf dist/{chrome,firefox,common,edge}/* build/*",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "pack": "npm run pack-chrome && npm run pack-firefox",
    "pack-chrome": "npm run build-chrome && crx pack ./dist/chrome --zip-output ./build/chrome.zip -o ./build/chrome.crx",
    "pack-firefox": "npm run build-firefox && web-ext build -a build -s dist/firefox -v"
  },
  "standard": {
    "global": [
      "chrome",
      "saveToClipboard",
      "canvasUtils",
      "location",
      "window",
      "document",
      "screen",
      "Image",
      "$",
      "jQuery",
      "FileReader",
      "MouseEvent",
      "KeyboardEvent"
    ]
  },
  "repository": "https://github.com/gyazo/gyazo-browser-extension.git",
  "author": "NOTA Inc <contact@gyazo.com>",
  "license": "MIT"
}
